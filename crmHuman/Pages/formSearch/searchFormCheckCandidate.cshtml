@if(@Model.RequestSearch !=null )
{
<form class="row g-3" method="get" action="/checkDuplicate">
    <div class="col-md-3">
        <label class="form-label">Số điện thoại/ Email</label>
        <input type="text"  value="@Model.RequestSearch.KeyCheck" name="KeyCheck" class="form-control">
    </div>
    <div class="d-flex justify-content-between">
       <button type="submit" class="btn btn-primary">Tìm kiếm</button>
    </div>
</form>
}
