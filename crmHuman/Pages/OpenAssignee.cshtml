  <div class="modal-header">
    <h5 class="modal-title">Phân case cho thông tin </h5>
    <button type="button" class="btn-close"
                    data-bs-dismiss="modal"
                    aria-label="Close"></button>
        </div>

        <div class="modal-body">
             <form id="mainForm2" class="needs-validation" novalidate>
                @if(@tiemObject.Id > 0)
                {
                 <div class="col-12 form-group">
                    <label class="form-label">Mã đơn hàng</label>
                      <input type="text" readonly value="@tiemObject.Code" name="fullName" id="txtCode" class="form-control" required placeholder="" />
                    <div class="invalid-feedback-cs">
                    </div>

                </div>
                }

                <div class="col-12 form-group">
                    <label class="form-label">Phân cho TC</label>
                    <select name="candidateId" id="cbAssingeeId" class="form-select" aria-label="">
                        <option value="-1">Chọn ứng cử viên</option>
                @foreach (var item in Model.allEmployers.Data)

                        {

                                  <option value="@item.Id">@item.FullName</option>
                        
                        }
                    </select>
                    <div class="invalid-feedback-cs">
                        Please choose a username.
                    </div>
                </div>
                



            </form>

        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary"
                    data-bs-dismiss="modal">
                Đóng
            </button>
    <button type="button" onclick="assingeeOrder(@tiemObject.Id)" class="btn btn-primary">Lưu</button>
        </div>

