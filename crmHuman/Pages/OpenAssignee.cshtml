            },
            new  Model.SelectDisplay()
            {
            Code ="2", Name ="TC"
            },
            new  Model.SelectDisplay()
            {
            Code ="3", Name ="TL"
            }
            , new Model.SelectDisplay()
            {
              Code ="4", Name ="Marketing"
            },
